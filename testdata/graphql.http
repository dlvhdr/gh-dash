###
# @env-stdin-cmd GH_TOKEN gh auth token

GRAPHQL https://api.github.com/graphql HTTP/1.1
Accept: application/json
Authorization: Bearer {{GH_TOKEN}}

query PullRequestChecks($url: URI!){
  resource(url: $url) {
    ... on PullRequest {
      commits(last: 1) {
        nodes {
          commit {
            statusCheckRollup {
              state
            }
          }
        }
      }
    }
  }
}

